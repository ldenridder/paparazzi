<!DOCTYPE module SYSTEM "module.dtd">

<module name="group_10_nav">
  <doc>
    <description>
    Exercise module for Autonomous Flight of MAV Course

    </description>
  </doc>
  <settings>
    <dl_settings>
      <dl_settings name="GROUP10NAV">
         <dl_setting var="max_velocity" min="0" step="0.1" max="1"/>
         <dl_setting var="soft_heading_rate" min="0" step="5" max="45" unit="rad" alt_unit="deg"/>
         <dl_setting var="medium_heading_rate" min="0" step="5" max="45" unit="rad" alt_unit="deg"/>
         <dl_setting var="hard_heading_rate" min="0" step="5" max="45" unit="rad" alt_unit="deg"/>
      </dl_settings>
    </dl_settings>
  </settings>
  
  <depends>cv_detect_color_object</depends>
  <header>
    <file name="group_10_nav.h"/>
  </header>
  <init fun="group_10_nav_init()"/>
  <periodic fun="group_10_nav_periodic()" freq="4"/>
  <makefile target="ap|nps">
    <file name="group_10_nav.c"/>
  </makefile>
</module>
  
